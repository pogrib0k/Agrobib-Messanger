<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Agrobibmessenger" parent="Theme.MaterialComponents.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00685B</item>
        <item name="colorPrimaryVariant">#439688</item>
        <item name="colorOnPrimary">#003D32</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#303F9F</item>
        <item name="colorSecondaryVariant">#666AD1</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Agrobibmessenger.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Agrobibmessenger.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Agrobibmessenger.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>